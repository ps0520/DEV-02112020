<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Order_Amt_Less_Shipping_Charges__c</fullName>
    <description>This field tracks Order amount without Shipping Charges</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Shipping_Charges__c), TotalAmount, (TotalAmount -  Shipping_Charges__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Order Amt Less Shipping Charges</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
