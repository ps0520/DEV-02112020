<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>G6_Instructions_Shipped__c</fullName>
    <description>Field checks if G6 IFUs were shipped out to the customer.</description>
    <externalId>false</externalId>
    <formula>IF(Account.G6_Instructions_Shipped__c, TRUE, (IF(Count_of_G6_Instructions__c &gt; 0, TRUE, FALSE)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>G6 Instructions Shipped</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
