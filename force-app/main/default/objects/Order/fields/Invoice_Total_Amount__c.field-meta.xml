<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Invoice_Total_Amount__c</fullName>
    <description>This field calculates the total amount including the copay to be displayed on the invoice</description>
    <externalId>false</externalId>
    <formula>Invoice_Gross_Price__c + Invoice_Insurant_Copay__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Invoice Total Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
